    "import pandas as pd\n",
    "\n",
    "def liczChunki(path):\n",
    "    suma = 0\n",
    "    for chunk in pd.read_csv(path, chunksize=50000):\n",
    "        suma += chunk[\"amount\"].sum()\n",
    "    return suma"
   ]
